extends layout.pug

block content

  h1 Books
  p
    a.button(href='/books/new') Create New Book
  table
    thead
      tr
        th Title
        th Author
        th Genre
        th Year
    tbody
    each val in book
      tr
        td 
          a(href='book_detail.html')= val.title
        td= val.author
        td= val.genre
        td= val.year
      

